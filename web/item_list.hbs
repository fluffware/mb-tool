<ul class="tag_list">
  {{#each this}}
  {{#with group}}
  <li class="tag_group">
    <div class="group_block">
      <div class="group_header">
	<img class="group_indicator" />
	<span class="group_addr">
	  {{> mb_addr}}
	</span>
	<span class="group_label">{{label}}</span>
      </div>
      <div class="group_body">
	{{#with children}}
	{{> item_list}}
	{{/with}}
      </div>
    </div>
  </li>
  {{/with}}
  {{#with tag}}
  <li class="tag_item">
    <span class="register_addr">{{>mb_addr}}</span>
    {{#with label}}
    <span class="register_label">{{this}}</span>
    {{/with}}
    <input type="
		 {{~#if (and (eq value_type 'integer') (eq radix 10))~}}
		 integer
		 {{~else~}}
		 {{~#if (eq value_type 'float')~}}
		 number
		 {{~else~}}
		 text
		 {{~/if}}
		 {{~/if~}}
		 "
	   class="mb_value"
	   mb:addr-low="{{addr_low}}"  mb:addr-high="{{addr_high}}"
	   mb:scale="{{scale}}" mb:radix="{{radix}}" mb:decimals="{{decimals}}"
	   {{#if unit}}mb:unit="{{unit}}"{{/if}}
	   value=""/>
  </li>
  {{/with}}
  {{/each}}
</ul>
